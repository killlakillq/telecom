    "@telecom/proto": "^0.0.1",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^20.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
