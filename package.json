{
  "name": "telecom",
  "version": "0.0.1",
  "description": "Telecom service with Asterisk PBX, event processing, and API functionality",
  "author": "",
  "private": true,
  "workspaces": [
    "libs/*",
    "apps/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "start": "lerna run start",
    "start:events-storage": "lerna run start --scope=@telecom/events-storage",
    "start:events": "lerna run start --scope=@telecom/events",
    "start:api": "lerna run start --scope=@telecom/api",
    "start:dev": "lerna run start:dev",
    "start:events-storage:dev": "lerna run start:dev --scope=@telecom/events-storage",
    "start:events:dev": "lerna run start:dev --scope=@telecom/events",
    "start:api:dev": "lerna run start:dev --scope=@telecom/api",
    "test": "lerna run test",
    "lint": "lerna run lint",
    "generate:proto": "lerna run generate --scope=@telecom/grpc",
    "clean:proto": "lerna run clean --scope=@telecom/grpc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lerna": "^8.2.1",
    "prettier": "^3.2.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  }
}
